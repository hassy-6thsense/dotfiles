set shell=/bin/bash\ -l\ -O\ expand_aliases
set number
set smartindent
set cindent
set showmode
set whichwrap=b,s,h,l,<,>,[,]
set nowrap
let loaded_matchparan=1
set backspace=indent,eol,start
set tabstop=4
set shiftwidth=4
set showmatch
let loaded_matchparen = 1

highlight SPACE cterm=underline ctermfg=lightred
match SPACE /ã€€/

set nobackup

set termencoding=utf-8
set encoding=utf-8
set fileencodings=ucs-bom,utf-8,iso-2022-jp,sjis,cp932,euc-jp,cp20932,latin1


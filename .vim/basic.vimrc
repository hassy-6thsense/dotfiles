set shell=/bin/bash\ -l\ -O\ expand_aliases
set number
set smartindent
set cindent
set showmode
set whichwrap=b,s,h,l,<,>,[,]
set nowrap
let loaded_matchparan=1
set backspace=indent,eol,start
set tabstop=4
